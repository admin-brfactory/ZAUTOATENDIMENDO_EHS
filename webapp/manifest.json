{
	"_version": "1.7.0",
	"sap.app": {
		"id": "arcelor.brZAUTOATENDIMENTO_EHS",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "arcelor.brZAUTOATENDIMENTO_EHS.view.App",
			"type": "XML",
			"async": true,
            "id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "arcelor.brZAUTOATENDIMENTO_EHS.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "arcelor.brZAUTOATENDIMENTO_EHS.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true,
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "",
				"name": "inicial_view",
				"target": ["InicialView"]
			}, {
				"pattern": "tipoColaboradorView",
				"name": "tipo_colaborador_view",
				"target": ["tipoColaboradorView"]
			},
			{	"pattern": "matriculaView",
				"name": "matricula_view",
				"target": ["matriculaView"]
			},{
					"pattern": "finalView",
				"name": "final_view",
				"target": ["finalView"]
			}],
			"targets": {
				"InicialView": {
					"viewId": "idInicialView",
					"viewName": "InicialView",
					"clearControlAggregation": false,
					"viewType": "XML"
				},
				"tipoColaboradorView": {
					"viewId": "idtipoColaboradorView",
					"viewName": "tipoColaboradorView",
					"clearControlAggregation": false,
					"viewType": "XML"
				},
				"matriculaView": {
					"viewId": "idmatriculaView",
					"viewName": "matriculaView",
					"clearControlAggregation": false,
					"viewType": "XML"
				},
				"finalView": {
					"viewId": "idfinalView",
					"viewName": "finalView",
					"clearControlAggregation": false,
					"viewType": "XML"
				}
			}
		}
	}
}