<mvc:View controllerName="arcelor.brZAUTOATENDIMENTO_EHS.controller.finalView" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:f="sap.f" xmlns:l="sap.ui.layout" xmlns:lf="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<Page showHeader="false" enableScrolling="false">
		<content>
			<OverflowToolbar id="otb1" class="finalOver">
				<Label width="41%"/>
				<Label text="{i18n>lblConcluido}" class="lblConclu"/>
				<ToolbarSpacer/>
			</OverflowToolbar>
			<VBox class="sapUiLargeMargin">
				<HBox>
					<Label wrapping="true" text="{finalModel>/mensagem}"/>
				</HBox>
			</VBox>
			<HBox height="3rem"/>
			<VBox alignItems="Center" justifyContent="End">
			<Button text="{i18n>lblOK}" type="Emphasized" width="20rem" class="TextStyle" press="onPress"/>
			</VBox>
		</content>
	</Page>
</mvc:View>